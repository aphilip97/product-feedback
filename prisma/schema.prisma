datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id       String @id @default(uuid())
  title    String @db.VarChar(50)
  category String
  status   String
  content  String @db.VarChar(250)

  @@map("post")
}
